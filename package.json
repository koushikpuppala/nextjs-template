	"name": "nextjs-template",
	"type": "module",
	"version": "0.2.5",
	"main": "next.config.ts",
	"packageManager": "pnpm@10.16.0",
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build --turbopack",
		"start": "next start",
