		"@prisma/extension-accelerate": "~2.0.2",
		"@sentry/nextjs": "~10.17.0",
		"@tanstack/react-table": "~8.21.3",
		"clsx": "~2.1.1",
		"date-fns": "~4.1.0",
		"dayjs": "~1.11.18",
		"firebase": "~12.3.0",
		"firebase-admin": "~13.5.0",
		"globby": "~15.0.0",
		"motion": "~12.23.22",
		"next": "~15.5.4",
		"react": "~19.2.0",
		"react-date-range": "~2.0.1",
		"react-dom": "~19.2.0",
		"react-icons": "~5.5.0",
		"sharp": "~0.34.4",
		"winston": "~3.18.3",
		"winston-daily-rotate-file": "~5.0.0",
		"zod": "~4.1.11",
		"zod-validation-error": "~4.0.2"
