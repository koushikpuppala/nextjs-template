{
	"name": "nextjs-template",
	"type": "module",
	"version": "0.1.0",
	"main": "next.config.ts",
	"packageManager": "pnpm@10.14.0",
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"prepare": "husky || true",
		"typecheck": "tsc --noEmit",
		"format": "prettier --check .",
		"format:fix": "prettier --write .",
		"postinstall": "prisma generate --schema=./src/prisma",
		"generate": "cross-env DOTENV_CONFIG_PATH=.env.development.local prisma generate --schema=./src/prisma",
		"studio": "cross-env DOTENV_CONFIG_PATH=.env.development.local prisma studio --browser none"
	},
	"license": "MIT",
	"bugs": "https://github.com/koushikpuppala/nextjs-template/issues",
	"repository": "https://github.com/koushikpuppala/nextjs-template.git",
	"dependencies": {
		"@ducanh2912/next-pwa": "~10.2.9",
		"@prisma/client": "~6.13.0",
		"@prisma/extension-accelerate": "~2.0.2",
		"@sentry/nextjs": "~10.0.0",
		"@tailwindcss/postcss": "~4.1.11",
		"babel-plugin-react-compiler": "19.1.0-rc.2",
		"clsx": "~2.1.1",
		"firebase": "~12.0.0",
		"firebase-admin": "~13.4.0",
		"globby": "~14.1.0",
		"motion": "~12.23.12",
		"next": "15.4.5",
		"postcss": "~8.5.6",
		"react": "~19.1.1",
		"react-dom": "~19.1.1",
		"react-icons": "~5.5.0",
		"tailwind-merge": "~3.3.1",
		"tailwindcss": "~4.1.11",
		"winston": "~3.17.0",
		"winston-daily-rotate-file": "~5.0.0",
		"zod": "~4.0.14",
		"zod-validation-error": "~4.0.1"
	},
	"devDependencies": {
		"@commitlint/cli": "~19.8.1",
		"@commitlint/config-conventional": "~19.8.1",
		"@commitlint/types": "~19.8.1",
		"@eslint/eslintrc": "~3.3.1",
		"@types/eslint": "~9.6.1",
		"@types/node": "~24.1.0",
		"@types/react": "~19.1.9",
		"@types/react-dom": "~19.1.7",
		"@typescript-eslint/parser": "~8.41.0",
		"cross-env": "~10.0.0",
		"cssnano": "~7.1.0",
		"eslint": "~9.32.0",
		"eslint-config-next": "~15.4.5",
		"eslint-config-prettier": "~10.1.8",
		"husky": "~9.1.7",
		"prettier": "~3.6.2",
		"prettier-plugin-prisma": "~5.0.0",
		"prettier-plugin-tailwindcss": "~0.6.14",
		"prisma": "~6.13.0",
		"sharp": "~0.34.3",
		"typescript": "~5.9.2"
	}
}
