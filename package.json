{
	"name": "@koushikpuppala/nextjs-template",
	"type": "module",
	"version": "0.0.0-development",
	"main": "next.config.ts",
	"packageManager": "pnpm@10.18.0",
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build --turbopack",
		"start": "next start",
		"lint": "eslint .",
		"prepare": "husky || true",
		"typecheck": "tsc --noEmit",
		"release": "semantic-release",
		"format": "prettier --check .",
		"format:fix": "prettier --write .",
		"studio": "prisma studio --browser none",
		"postinstall": "prisma generate --no-engine"
	},
	"lint-staged": {
		"*.{js,ts,tsx,jsx,json,css,md,yml}": [
			"eslint",
			"prettier --write"
		]
	},
	"license": "MIT",
	"bugs": "https://github.com/koushikpuppala/nextjs-template/issues",
	"repository": {
		"type": "git",
		"url": "https://github.com/koushikpuppala/nextjs-template.git"
	},
	"author": "Koushikpuppala <contact@koushikpuppala.com> (https://koushikpuppala.com)",
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"nextjs",
		"template",
		"starter",
		"boilerplate"
	],
	"dependencies": {
		"@ducanh2912/next-pwa": "~10.2.9",
		"@headlessui/react": "~2.2.9",
